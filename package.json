{
  "name": "empty-project-template",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "lint": "node_modules/.bin/eslint --ext .js ./",
    "storybook": "REACT_NATIVE_STORYBOOK=true expo start",
    "prettier": "prettier --ignore-path ./.gitignore --write src/**/{.,*}.{js,css} *.{md,json} .storybook/**",
    "test": "jest --no-watchman"
  },
  "dependencies": {
    "@expo/vector-icons": "^8.1.0",
    "expo": "^30.0.1",
    "native-base": "^2.10.0",
    "prop-types": "^15.6.2",
    "react": "16.3.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-30.0.0.tar.gz",
    "react-redux": "^5.1.0",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "@storybook/addon-actions": "^4.0.2",
    "@storybook/addon-links": "^4.0.2",
    "@storybook/addon-storyshots": "^4.0.2",
    "@storybook/addons": "^4.0.2",
    "@storybook/react-native": "^4.0.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.0",
    "husky": "^1.1.3",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.49.0",
    "prettier": "^1.14.3",
    "prop-types": "^15.6.2",
    "react-dom": "16.3.1",
    "react-test-renderer": "^16.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier && yarn lint --fix && yarn test && git diff --exit-code"
    }
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|react-native-button)/)"
    ]
  }
}
